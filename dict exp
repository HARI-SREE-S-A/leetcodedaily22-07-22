nums = [1,1,1,2,2,2,3]
k = 2
dicct = {}
new = {}

for i,n in enumerate(nums):
    s = n
    if s not in dicct:
        dicct[s] = nums.count(s)
    #else:
        #dicct[s].append(s)
values = dicct.items()
sorted_values = dict(sorted(dicct.items(),key = lambda item:item[1]))
for value in new:

    new[value] = dicct[value]
print(sorted_values)



